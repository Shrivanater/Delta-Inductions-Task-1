#include<iostream>
#include<string.h>

using namespace std;

const int N = 1e6;

int main(){

    int l, i, count0 = 0, count1 = 0;
    char bin[N];
    
    cin >> l;
    cin >> bin;
    
    for(i = 0; i < l; i++){
        if(bin[i] == '0')
            count0++;
        else
            count1++;
    }
    
    if(count0 == l || count1 == l){
        cout << -1;
        exit(0);
    }
    
    if(bin[l-1] == '1'){
        
        bin[l-1] = '0';
        cout << bin << " ";
        bin[l-1] = '1';
        
        for(i = l-1; i >= 0; i--){
            if(bin[i] == '1')
                bin[i] = '0';
            else{
                bin[i] = '1'; 
                break;
            }
        }
                
        cout << bin;
    }
    
    else{
        
        char temp[N];
        strcpy(temp, bin);
        
        for(i = l-1; i >= 0; i--){
            if(bin[i] == '0')
                bin[i] = '1';
            else{
                bin[i] = '0'; 
                break;
            }
        }
        
        cout << bin << " ";
        
        temp[l-1] = '1';
        cout << temp;
    }
}
    
